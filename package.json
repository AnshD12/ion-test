{
  "name": "ion-test",
  "version": "1.0.0",
  "description": "full stack assignment for ion energy",
  "main": "index.js",
  "scripts": {
    "test": "mocha --timeout 10000",
    "lint": "./node_modules/.bin/eslint .",
    "fix": "./node_modules/.bin/eslint --fix .",
    "server": "./node_modules/.bin/nodemon index.js",
    "client": "cd client && ng serve --open",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnshD12/ion-test.git"
  },
  "author": "Anshul Dharmadhikari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnshD12/ion-test/issues"
  },
  "homepage": "https://github.com/AnshD12/ion-test#readme",
  "nodemonConfig": {
    "ignore": [
      "app/uploads/*.json"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "mocha": "^10.2.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "mongoose": "^5.5.3",
    "multer": "^1.4.2",
    "winston": "^3.2.1"
  }
}